<?xml version="1.0" encoding="UTF-8"?>

<!-- Dieses Schema dient als Containerelement aller Episoden einer bestimmten Season-->

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<!-- Master inkludieren -->
	<xs:include schemaLocation="Serientracker.xsd"/>

	<!-- Sammlung zugehÃ¶riger Episoden einer Season -->
	<xs:element name="episodes">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="episode" minOccurs="0" maxOccurs="25"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

</xs:schema>

