<?xml version="1.0" encoding="UTF-8"?>

<!-- Dieses Schema dient als Containerelement aller Listen-->

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">


	<!-- Sammlung aller Listen -->
	<xs:element name="lists">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="listID" maxOccurs="unbounded"/>
			</xs:sequence>

		</xs:complexType>
	</xs:element>


	<!-- globale listID Zuordnung -->
	<xs:element name="listID" type="xs:positiveInteger"/>	
	
	

</xs:schema>
