<?xml version="1.0" encoding="UTF-8"?>

<!-- Dieses Schema definiert ein Wurzelelement, dass alle User aufnimmt.-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">


	<!--Container der alle User beinhaltet-->
	<xs:element name="users">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="userID" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="userID" type="xs:positiveInteger"/>

</xs:schema>
