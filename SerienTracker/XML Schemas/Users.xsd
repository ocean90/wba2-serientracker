<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://Serientracker">

<!-- Dieses Schema definiert ein Wurzelelement, dass alle User aufnimmt.->


<!-- Container der alle User beinhaltet -->
<xs:element name="users">
  <xs:complexType>
    <xs:sequence>
      <xs:element ref="user" maxOccurs="unbpunded"/>
    </xs:sequence>

  </xs:complexType>
  
</xs:element>


</xs:schema>