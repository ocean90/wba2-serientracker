<?xml version="1.0" encoding="UTF-8"?>

<!-- Dieses Schema dient der Sammlung aller vorhandenen Serien -->

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<!-- Schema fÃ¼r einzelne Serie inkludieren -->
	<xs:include schemaLocation="Serie.xsd"/>

	<!-- Element das alle Serien aufnimmt -->
	<xs:element name="series">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="serie" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

</xs:schema>
